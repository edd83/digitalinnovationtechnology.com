{
  "version": "2.0.0",
  "author": {
    "name": "Eddy Lardet"
  },
  "description": "React boilerplate with ES7, Express.js, Webpack and airbnb linter",
  "license": "MIT",
  "engines": {
    "node": ">8.11.1",
    "npm": ">6.4.1"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node src/server",
    "start": "cross-env NODE_ENV=production pm2 start build/server",
    "prebuild": "rimraf build",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --config=config/webpack.config.prod.js",
    "build:server": "copyfiles -a -u 1 src/server/**/**/* build",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ]
  },
  "postcss": {},
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.1.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "babel-watch": "2.0.7",
    "copyfiles": "2.1.0",
    "css-loader": "1.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-fbjs": "2.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-relay": "0.0.28",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "immutable": "3.8.2",
    "intl": "1.2.5",
    "jest": "23.6.0",
    "jest-css-modules-transform": "^2.0.3",
    "jest-enzyme": "6.1.2",
    "jest-fetch-mock": "^1.6.6",
    "node-sass": "4.9.3",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.10",
    "prettier": "1.14.3",
    "react-hot-loader": "4.3.11",
    "react-intl": "2.7.0",
    "rimraf": "2.6.2",
    "sanitize.css": "7.0.3",
    "sass-loader": "7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.0",
    "typescript": "^3.1.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.2",
    "webpack-merge": "4.1.4",
    "whatwg-fetch": "3.0.0"
  },
  "dependencies": {
    "@types/intl": "^1.2.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.8",
    "@types/react-intl": "^2.3.11",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "express": "4.16.3",
    "fast-safe-stringify": "2.0.6",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "winston": "3.1.0"
  }
}
